<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.CustomerListController">
   <HBox alignment="BOTTOM_LEFT" prefHeight="25.0" prefWidth="402.0">
      <children>
            <Button mnemonicParsing="false" onAction="#onGobackClick" text="Go back" />
      </children>
   </HBox>
    <TableView fx:id="customerTableView">
        <columns>
            <TableColumn fx:id="usernameColumn" text="Username" />
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="emailColumn" text="Email" />
            <TableColumn fx:id="addressColumn" text="Address" />
            <TableColumn fx:id="actionColumn" text="Actions" />
        </columns>
    </TableView>
</VBox>
