<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #e8ebe4;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.AvailableCarsListController">
    <children>
        <TableView fx:id="carsTableView" layoutX="43.0" layoutY="61.0" prefHeight="500.0" prefWidth="715.0">
            <columns>
                <TableColumn fx:id="regnoColumn" text="Reg No" />
                <TableColumn fx:id="brandColumn" text="Brand" />
                <TableColumn fx:id="modelColumn" text="Model" />
                <TableColumn fx:id="statusColumn" text="Status" />
                <TableColumn fx:id="priceColumn" text="Price" />
                <TableColumn fx:id="actionColumn" text="Action" />
            </columns>
        </TableView>
        <HBox alignment="CENTER" layoutX="10.0" layoutY="520.0" spacing="10" />
          <Button layoutX="43.0" layoutY="14.0" mnemonicParsing="false" onAction="#onGobackClick" style="-fx-background-color: #CC8366;" text="Go back" textFill="#e8ebe4" />
    </children>
</Pane>
