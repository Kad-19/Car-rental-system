            </columns>
        </TableView>
        <Button layoutX="73.0" layoutY="20.0" mnemonicParsing="false" onAction="#onGobackClick" style="-fx-background-color: #CC8366;" text="Go back" textFill="#e8ebe4" />
    </children>
