                <TableColumn fx:id="regnoColumn" prefWidth="112.0" text="Reg No" />
                <TableColumn fx:id="priceColumn" prefWidth="94.0" text="Price" />
                <TableColumn fx:id="startDateColumn" prefWidth="120.0" text="Start Date" />
                <TableColumn fx:id="endDateColumn" prefWidth="114.0" text="End Date" />
